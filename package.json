{
  "name": "opentokjs",
  "version": "1.0.0",
  "description": "OpenTok javascript and typescript module using nodejs.",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "scripts": {
    "debug": "cross-env NODE_ENV=development ts-node -r ./babel.register.js ./src/main.ts",
    "test": "cross-env NODE_ENV=development mocha -r ./babel.register.js ./tests/*.ts",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config=./webpack.config.js --https --open --hot",
    "build": "cross-env NODE_ENV=production webpack --config=./webpack.config.js --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://icaksama@github.com/DynEd/OpenTokJS.git"
  },
  "keywords": [
    "opentok"
  ],
  "author": "icaksama",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DynEd/OpenTokJS/issues"
  },
  "homepage": "https://github.com/DynEd/OpenTokJS#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@opentok/client": "^2.17.3",
    "@types/mocha": "^7.0.1",
    "@types/opentok": "^2.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.0.5",
    "file-loader": "^6.0.0",
    "mocha": "^7.0.1",
    "opentok": "^2.10.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2"
  },
  "dependencies": {}
}
